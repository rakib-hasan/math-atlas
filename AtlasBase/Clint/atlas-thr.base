 * Don't wakeup/create threads if being called for serial execution: just
 * do work on master process to save overhead & ease debugging
 */
   if (P0 < 2)

   {
      if (!ATL_TP_PTR)
         ATL_TP1_PTR = ATL_NewThreadPool(1, 0, NULL);
      pp = ATL_TP1_PTR;
      pp->nworkers = 1;
      pp->PD = PD;
      pp->extra = extra;
      pp->jobf = DoWork;
      pp->combf = DoComb;
