/*
 *    Wait for thread to get into busy-loop
 */
      while (pp->chkin[i] != 3)
         ATL_thread_yield();
/*
 *    Tell ready thread to start working
 */
      pp->chkin[i] = 1;
   }
   ATL_tpool_dojob(pp, 0, 0);
/*
 * Wait for all threads to complete
 */
   for (i=1; i < P; i++)
   {
      while (pp->chkin[i] != 3)
         ATL_thread_yield();
   }
