/*
 * When pool finished, await everyone to quit, then free the pool
 */
   while(pp->nthr)
      ATL_thread_yield();
   
   ATL_TP_PTR = NULL;
   ATL_FreeThreadPool(pp);
   if (ATL_TP1_PTR)
   {
      ATL_FreeThreadPool(ATL_TP1_PTR);
      ATL_TP1_PTR = NULL;
   }
