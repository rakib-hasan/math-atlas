   int nbf;          /* N%nb, if 0, nb */
   int nnuf;         /* CEIL(nbf/nu) */
   int nmuf;         /* CEIL(nbf/mu) */
   int kb;           /* blocking used for K */
   int kb0;          /* K%kb, if 0, kb */
   int KB0;          /* if kmajor, it is CEIL(kb0/ku)*ku, else kb0 */
   int blkszA;       /* nb*kb */
