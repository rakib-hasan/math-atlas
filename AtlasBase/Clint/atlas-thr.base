   const int eltsh = ptmms[0].eltsh, kb = ptmms[0].kb, m = ptmms[0].mb*Mblks+mr,
             n = ptmms[0].nb*Nblks+nr, minblks = Kblks / P, 
             extrablks = Kblks - minblks*P;

   for (p=i=0; i < P; i++)
   {
